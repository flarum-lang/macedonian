{
	"name": "flarum-lang/macedonian",
	"type": "flarum-extension",
	"description": "Macedonian language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/macedonian",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"mk-locale",
		"macedonian-locale",
		"macedonian"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/macedonian/issues",
		"source": "https://github.com/flarum-lang/macedonian"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Macedonian",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "mk",
			"title": "Macedonian"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
